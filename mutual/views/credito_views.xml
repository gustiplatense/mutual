<odoo>
	<record id="view_credito_tree" model="ir.ui.view">
		<field name="name">credito.list</field>
		<field name="model">mutual.credito</field>
		<field name="arch" type="xml">
			<list>
				<field name="dni"/>
				<field name="nombre_afiliado"/>
				<field name="empleador_afiliado"/>
				<field name="nro_credito"/>
				<field name="cuota_actual"/>
				<field name="monto"/>
				<field name="monto_solicitado"/>
				<field name="comercio"/>
			</list>
		</field>
	</record>
</odoo>