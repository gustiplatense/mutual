<odoo>
	<record id="view_credito_importador_form" model="ir.ui.view">
		<field name="name">credito.importador.form</field>
		<field name="model">mutual.credito.importador</field>
		<field name="arch" type="xml">
			<form string="Importar Creditos desde TXT">
				<group>
					<field name="archivo_txt"/>
					<field name="nombre_archivo"/>
				</group>
				<footer>
					<button string="Importar" type="object" name="importar_creditos" class="btn-primary"/>
					<button string="Cancelar" class="btn-secondary" special="cancel"/>
				</footer>
			</form>
		</field>
	</record>
</odoo>